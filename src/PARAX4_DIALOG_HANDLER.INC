        IF(MESSAGE%WIN.EQ.IDD_FIRD) THEN
        CALL WDIALOGSELECT(IDD_FIRD)
           SELECT CASE(MESSAGE%VALUE1)
            CASE (IDOK)
        CALL WDIALOGGETINTEGER(IDF_FIRD1,ISET)
        CALL WDIALOGGETINTEGER(IDF_FIRD2,JSET)
        CALL WDIALOGGETDOUBLE(IDF_FIRD3,WAVE_LENGTH)
        WRITE(INPUT,*) 'FIRD ',ISET,JSET,WAVE_LENGTH
        CALL PROCES
        WRITE(OUTLYNE,*) '   '
        CALL SHOWIT(0)
        CASE(IDCANCEL)
        CALL WDialogHide()
        CALL WDialogUnload()
        END SELECT
                                END IF
